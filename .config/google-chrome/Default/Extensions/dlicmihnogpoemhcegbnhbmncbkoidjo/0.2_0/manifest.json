{
   "background": {
      "scripts": [ "background.js", "jquery.min.js" ]
   },
   "description": "Shows a page action for imdb/rotten tomatoes film pages to go to the letterboxd equivalant",
   "icons": {
      "128": "icon-128.png",
      "48": "icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Letterboxd Movie Assistant",
   "page_action": {
      "default_icon": "icon-19.png",
      "default_title": "Go to this films letterboxd page!"
   },
   "permissions": [ "tabs", "http://api.rottentomatoes.com/api/public/v1.0*", "http://www.rottentomatoes.com/*", "http://www.imdb.com/*", "http://letterboxd.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2"
}
